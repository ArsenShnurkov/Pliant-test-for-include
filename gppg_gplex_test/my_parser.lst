%scanbasetype ScanBase  //names the ScanBaseclass to "ScanBase"
%using Scanner      // include the Namespace of the scanner-class

%namespace Parser   // names the namespace of the Parser-class
%parsertype Parser      //names the Parserclass to "Parser"

%tokentype Tokens       //names the Tokensenumeration to "Tokens"

// Token definitions for GPLEX
%token SPACE
%token EOL
%token WORD

%union { 
		public int iVal;
		public string sVal;
}

%% //Grammar Rules Section
// Error: There are 4 non-terminating NonTerminal Symbols
   //  {line, parts, space_parts, word_parts}
// Warning: The following 2 symbols form a non-terminating cycle 
   // {word_parts, space_parts}
// Warning: Terminating word_parts fixes the following size-4 NonTerminal set
   // {line, parts, space_parts, word_parts}
// Warning: Terminating space_parts fixes the following size-4 NonTerminal set
   // {line, parts, space_parts, word_parts}
// ---------------------------------------------------------------------------

file
    : /* nothing */
	| file_parts
    ;

file_parts
    : file_part
	| file_part file_parts
    ;

file_part
	: line
	| EOL
	;

line
	: parts
    ;

parts
	: space_parts
	| word_parts
	;

space_parts
	: SPACE word_parts
	;

word_parts
	: WORD space_parts
	;

